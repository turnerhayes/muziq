{
	"extends": [
		"../.eslintrc.json",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"settings": {
		"react": {
			"version": "16.4"
		}
	},
	"env": {
		"browser": true,
		"node": false
	},
	"rules": {
		"jsx-quotes": "error"
	}
}
